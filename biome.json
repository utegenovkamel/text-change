{
	"formatter": {
		"enabled": true,
		"formatWithErrors": false,
		"indentStyle": "space",
		"indentWidth": 2,
		"lineWidth": 80,
		"lineEnding": "lf",
		"ignore": []
	},
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"suspicious": {
				"noExplicitAny": "warn",
				"noArrayIndexKey": "off"
			},
			"style": {
				"noUselessElse": "off",
				"useSelfClosingElements": "warn",
				"useLiteralEnumMembers": "off"
			}
		}
	}
}
